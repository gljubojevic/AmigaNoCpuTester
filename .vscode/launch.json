	"version": "0.2.0",
	"configurations": [
		{
			"type": "amiga-assembly",
			"request": "launch",
			"name": "FS-UAE A500+ Launch",
			"noDebug": true,
			"program": "${workspaceFolder}/uae/dh0/NoCpuTester",
			"remoteProgram": "SYS:NoCpuTester",
			"emulatorType": "fs-uae",
			"emulatorArgs": [
				"--hard_drive_0=${workspaceFolder}/uae/dh0",
				"--automatic_input_grab=0",
				"--amiga_model=A500+",
				"--chip_memory=512",
				"--slow_memory=512",
				"--joystick_port_1=none",
				"--video_sync=off"
			],
			"preLaunchTask": "amigaassembly: build"
		},
